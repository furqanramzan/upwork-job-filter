    "@nuxtjs/tailwindcss": "^6.8.0",
    "@trpc/client": "^10.38.4",
    "@trpc/server": "^10.38.4",
    "@types/node": "^20",
    "@vite-pwa/nuxt": "^0.1.1",
    "@vueuse/nuxt": "^10.4.1",
    "drizzle-kit": "^0.19.13",
    "drizzle-orm": "^0.28.6",
    "drizzle-zod": "^0.5.1",
    "element-plus": "^2.3.14",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "mysql2": "^3.6.1",
    "nuxt": "^3.7.0",
    "nuxt-scheduler": "^0.1.9",
    "pm2": "^5.3.0",
