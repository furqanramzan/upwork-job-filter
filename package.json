{
  "name": "upwork-job-filter",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "fmt": "prettier --write .",
    "lint": "eslint --fix --max-warnings 0 .",
    "prepare": "nuxt prepare",
    "migration": "drizzle-kit generate:mysql --schema=./server/drizzle/schema.ts --out=./server/drizzle/migrations",
    "stop": "pm2 stop upworkjobfilter || true",
    "prod": "npm run stop && rm -rf .output/tmp && pm2 start",
    "buildprod": "npm run stop && npm run build && npm run prod"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
